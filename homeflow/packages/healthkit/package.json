{
  "name": "@spezivibe/healthkit",
  "version": "1.0.0",
  "description": "Apple HealthKit integration for React Native with configurable health metrics",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist *.tsbuildinfo",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run clean && npm run build && npm run test"
  },
  "keywords": [
    "react-native",
    "healthkit",
    "apple-health",
    "health-data",
    "ios",
    "spezi"
  ],
  "author": "SpeziVibe",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.70.0",
    "@kingstinct/react-native-healthkit": ">=9.0.0",
    "expo": ">=52.0.0",
    "expo-constants": ">=17.0.0"
  },
  "peerDependenciesMeta": {
    "@kingstinct/react-native-healthkit": {
      "optional": false
    }
  },
  "devDependencies": {
    "@kingstinct/react-native-healthkit": "^9.0.0",
    "expo": "~54.0.30",
    "expo-constants": "~17.1.8",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@react-native/babel-preset": "^0.81.5",
    "@testing-library/react-native": "^12.9.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.17",
    "@types/react-test-renderer": "^18.0.7",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "react": "^19.1.0",
    "react-native": "^0.81.5",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "jest": {
    "preset": "react-native",
    "testTimeout": 15000,
    "setupFiles": [
      "./jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|expo|expo-constants|@testing-library)/)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.ts?(x)"
    ]
  }
}
